<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="cacheEnabled" value="false"/>
        <setting name="useGeneratedKeys" value="true"/>
        <setting name="defaultExecutorType" value="REUSE"/>
        <setting name="jdbcTypeForNull" value="VARCHAR"/>
    </settings>
    
    <typeAliases>
        <typeAlias type="vo.MemberVO" alias="member"/>
        <typeAlias type="vo.PetVO" alias="pet"/>
        <typeAlias type="vo.StatisticsVO" alias="statistics"/>
        <typeAlias type="vo.HealthVO" alias="helath"/>
        <typeAlias type="vo.BoardVo" alias="board"/>
        <typeAlias type="vo.ReplyVo" alias="reply"/>
        <typeAlias type="vo.WeightVo" alias="weight"/>
        <typeAlias type="vo.StollVo" alias="stoll"/>
        <typeAlias type="vo.NoteVo" alias="note"/>
        <typeAlias type="vo.FeedingVo" alias="feeding"/>
        <typeAlias type="vo.NewsItem"  alias="newsitem"/>
    </typeAliases>
    
    <mappers>
        <mapper resource="config/mybatis/mapper/member.xml"/>
        <mapper resource="config/mybatis/mapper/board.xml"/>
        <mapper resource="config/mybatis/mapper/pet.xml"/>
        <mapper resource="config/mybatis/mapper/statistics.xml"/>
        <mapper resource="config/mybatis/mapper/health.xml"/>
        <mapper resource="config/mybatis/mapper/diary.xml"/>
		<mapper resource="config/mybatis/mapper/board.xml"/>
		<mapper resource="config/mybatis/mapper/reply.xml"/>
		<mapper resource="config/mybatis/mapper/answer.xml"/>
		<mapper resource="config/mybatis/mapper/diary.xml" />
		<mapper resource="config/mybatis/mapper/newsitem.xml" />
    </mappers>
</configuration>
